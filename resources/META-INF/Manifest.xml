<idea-plugin version="2">
	<extensions defaultExtensionNs="org.consulo.java">
		<manifest.headerParser key="" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser" order="last"/>

		<manifest.headerParser key="Created-By" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Signature-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Class-Path" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Implementation-Title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Implementation-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Implementation-Vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Implementation-Vendor-Id" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Implementation-URL" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Specification-Title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Specification-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Specification-Vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Sealed" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Content-Type" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Java-Bean" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="MD5-Digest" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="SHA-Digest" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Magic" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Name" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Manifest-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>

		<!--TODO [VISTALL] this is list of futures - that need replace to other plugins-->
		<!--[JavaFX] JavaFX futures-->
		<manifest.headerParser key="JavaFX-Application-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="JavaFX-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="JavaFX-Class-Path" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Main-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="implementation-vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="implementation-title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="implementation-version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="JavaFX-Preloader-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="JavaFX-Fallback-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>

		<!--[SpringDM] Spring futures-->
		<manifest.headerParser key="Spring-Context" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Import-Library" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Library-SymbolicName" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Library-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Library-Name" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Library-Description" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Import-Bundle" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="Application-TraceLevels" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
		<manifest.headerParser key="SpringExtender-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Module-Type" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Web-ContextPath" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
		<manifest.headerParser key="Web-DispatcherServletUrlPatterns" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<localInspection shortName="MissingFinalNewlineInspection" displayName="Missing Final New Line" groupName="Manifest" enabledByDefault="true" level="ERROR"
						 implementationClass="org.osmorc.manifest.codeInspection.MissingFinalNewlineInspection"/>

		<colorSettingsPage implementation="org.osmorc.manifest.lang.ManifestColorsAndFontsPage"/>
		<errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
		<lang.parserDefinition language="MANIFEST" implementationClass="org.osmorc.manifest.lang.ManifestParserDefinition"/>
		<completion.contributor language="MANIFEST" implementationClass="org.osmorc.manifest.ManifestCompletionContributor"/>
		<lang.syntaxHighlighterFactory key="MANIFEST" implementationClass="org.osmorc.manifest.lang.ManifestSyntaxHighlighterFactory"/>
		<annotator language="MANIFEST" implementationClass="org.osmorc.manifest.lang.headerparser.HeaderAnnotator"/>
		<annotator language="MANIFEST" implementationClass="org.osmorc.manifest.lang.ManifestHighlightingAnnotator"/>
		<lang.elementManipulator forClass="org.osmorc.manifest.lang.psi.HeaderValuePart" implementationClass="org.osmorc.manifest.lang.psi.HeaderValuePartManipulator"/>
		<lang.ast.leafFactory implementation="org.osmorc.manifest.lang.psi.impl.ManifestASTLeafFactory"/>
		<fileTypeFactory implementation="org.osmorc.manifest.lang.ManifestFileTypeFactory"/>
		<fileEditorProvider implementation="org.osmorc.manifest.editor.ManifestEditorProvider"/>
		<lang.versionResolver language="MANIFEST" implementationClass="org.osmorc.manifest.lang.ManifestLanguageVersionResolver"/>
	</extensions>
</idea-plugin>